<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class table="wordpress" name="MySQL.Domain.WordPress, MySQL" lazy="false">
    <id name="Id" unsaved-value="0" type="long" column="ID">
      <generator class="identity" />
    </id>
    <property name="FName" column="Fname" not-null="true"></property>
    <property name="LName" column="Lname" not-null="true"></property>
    <property name="Email" column="Email" not-null="true"></property>
    <property name="Username" column="Username" not-null="true"></property>
    <property name="Password" column="Password" not-null="true"></property>
    <property name="Activated" column="Activated" not-null="true"></property>
    <property name="BlogUrl" column="BlogUrl" not-null="true"></property>
  </class>
</hibernate-mapping>